{
  "extends": [
    "plugin:import/errors",
    "plugin:import/warnings",
    "prettier",
    "airbnb-base"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": ["error", "fb"],
    "max-len": ["error", {"code": 120, "ignoreUrls": true}],
    "no-confusing-arrow": "error",
    "no-mixed-operators": "error",
    "no-unexpected-multiline": "error"
  }
}
